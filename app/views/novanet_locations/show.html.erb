<%- model_class = NovanetLocation -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>


<% NovanetLocationEnrollment.by_location(1).each do |enrollment| %>
<div> <%= Student.find(enrollment.student_id).first_name %>  <%= Student.find(enrollment.student_id).last_name %> </div>
<% end %>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              novanet_locations_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_novanet_location_path(@novanet_location), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              novanet_location_path(@novanet_location),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
