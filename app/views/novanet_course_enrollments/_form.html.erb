<%= form_for @novanet_course_enrollment, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
    <%= f.label :student_id, :class => 'control-label' %>
    <div class="controls">
            <%= f.collection_select :student_id, Student.all.order(:last_name), :id, :name, {}, {:class => 'dropdown-toggle'} %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :NovanetCourse_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.collection_select :NovanetCourse_id, NovanetCourse.all.order(:name), :id, :name, {}, {:class => 'dropdown-toggle'} %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                novanet_course_enrollments_path, :class => 'btn' %>
  </div>
<% end %>
