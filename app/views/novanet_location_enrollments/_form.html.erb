<%= form_for @novanet_location_enrollment, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
    <%= f.label :student_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.collection_select :student_id, Student.all.order(:last_name), :id, :name, {}, {:class => 'dropdown-toggle'} %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label "Novanet Location", :class => 'control-label' %>
    <div class="controls">
      <%= f.collection_select :novanet_location_id, NovanetLocation.all, :id, :name, {}, {:class => 'dropdown-toggle'} %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.submit "Assign", :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                novanet_location_enrollments_path, :class => 'btn' %>
  </div>
<% end %>
