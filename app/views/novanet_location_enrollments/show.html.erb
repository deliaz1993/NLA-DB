<%- model_class = NovanetLocationEnrollment -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:student_id) %>:</strong></dt>
  <dd><%= @novanet_location_enrollment.student_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:NovanetLocation_id) %>:</strong></dt>
  <dd><%= @novanet_location_enrollment.NovanetLocation_id %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              novanet_location_enrollments_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_novanet_location_enrollment_path(@novanet_location_enrollment), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              novanet_location_enrollment_path(@novanet_location_enrollment),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
